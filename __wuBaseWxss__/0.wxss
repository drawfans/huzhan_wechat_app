.ly {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1001;
    overflow: hidden;
    width: 100%;
    height: 100%;
    border-radius: 3px;
    text-align: center;
}

.ly.hide,.ly_bg.hide {
    display: none;
}

.ly:before {
    display: inline-block;
    margin-right: -.25em;
    height: 100%;
    content: '';
    vertical-align: middle;
}

.ly .content {
    display: inline-block;
    overflow: hidden;
    overflow-y: auto;
    max-width: 100%;
    max-height: 100%;
    min-width: 50%;
    border-radius: 3px;
    background: #fff;
    vertical-align: middle;
}

.ly.show .content {
    animation-duration: .3s;
    animation-name: bounceIn;
}

@-webkit-keyframes bounceIn {
    0% {
        opacity: 0;
        transform: scale(.5);
    }

    100% {
        opacity: 1;
        transform: scale(1);
    }
}

@keyframes bounceIn {
    0% {
        opacity: 0;
        transform: scale(.5);
    }

    100% {
        opacity: 1;
        transform: scale(1);
    }
}

